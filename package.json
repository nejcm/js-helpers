  "author": "Nejc Mursic <nmursi2@gmail.com> (https://github.com/nejcm)",
  "description": "Javascript utilities and helper functions",
  "source": "src/index.ts",
  "main": "dist/@nejcm/js-helpers.cjs.js",
  "umd:main": "dist/@nejcm/js-helpers.umd.js",
  "module": "dist/@nejcm/js-helpers.esm.js",
  "types": "types/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "repository": {
    "type": "git",
