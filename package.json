  },
  "bugs": {
    "url": "https://github.com/nejc/js-helpers/issues"
  },
  "homepage": "https://github.com/nejcm/js-helpers",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "keywords": [
    "js",
    "helpers",
    "utility",
    "functions",
    "es6",
    "react",
    "library"
  ],
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o .storybook-static",
    "storybook:deploy": "npm run storybook:build && gh-pages -d .storybook-static",
    "format": "prettier ./src/**/* --write",
    "clean": "rimraf dist coverage .storybook-static",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "npm run lint && jest",
    "test:cov": "jest --coverage --colors && jest-coverage-badges",
    "test:build": "jest --coverage --colors --silent && jest-coverage-badges",
    "test:watch": "jest --watch",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "tsc:declaration": "rimraf types/* && tsc -p tsconfig.json --emitDeclarationOnly",
    "build": "npm run clean && kcd-scripts build --bundle --no-clean --size-snapshot && npm run tsc:declaration",
    "validate": "npm run format && npm run lint && npm run test:build",
    "setup": "npm install && npm run validate",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
